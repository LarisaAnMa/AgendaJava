public class NodoTelefonos {

	private String info;
	private NodoTelefonos liga;

	public NodoTelefonos() {

	}

	public NodoTelefonos(String info) {
		this.info = info;
		this.liga = null;
	}

	public String getInfo() {
		return info;
	}

	public void setInfo(String info) {
		this.info = info;
	}

	public NodoTelefonos getLiga() {
		return liga;
	}

	public void setLiga(NodoTelefonos liga) {
		this.liga = liga;
	}

}
